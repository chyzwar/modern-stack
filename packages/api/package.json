{
  "name": "api",
  "version": "1.0.0",
  "description": "API",
  "scripts": {
    "prestart": "yarn build && yarn db:migrate",
    "start": "env-cmd -f .env.local nodemon dist/server.js --inspect=0.0.0.0:9231",
    "start:prod": "env-cmd -f .env.production node dist/server.js --inspect=0.0.0.0:9231",
    "build": "tsc --build",
    "build:watch": "tsc --build --watch",
    "db:seed": "env-cmd -f .env.local sequelize db:seed",
    "db:migrate": "env-cmd -f .env.local sequelize db:migrate"
  },
  "license": "UNLICENSED",
  "dependencies": {
    "fastify": "2.x.x", 
    "fastify-jwt": "1.x.x",
    "pino": "5.x.x",
    "pino-pretty": "3.x.x",
    "sequelize": "5.x.x",
    "pg": "7.x.x",
    "pg-hstore": "2.x.x"
  },
  "devDependencies": {
    "@types/jest": "24.x.x",
    "@types/node": "12.x.x",
    "@types/pino": "5.x.x",
    "@typescript-eslint/parser": "2.x.x",
    "@typescript-eslint/eslint-plugin": "2.x.x",
    "sequelize-cli": "5.x.x",
    "env-cmd": "10.x.x",
    "nodemon": "2.x.x",
    "jest": "24.x.x",
    "ts-node": "8.6.2",
    "ts-jest": "24.x.x",
    "typescript": "3.8.x", 
    "eslint": "6.x.x",
    "eslint-plugin-jest": "23.x.x",
    "eslint-plugin-import": "2.x.x"
  }
}
